                            <% if(sellingListOpen.length == 0) {%>
                                <br><br>
                                <div align="center"><h3>No Items to display. Check other sections.</h3></div>
                            <% } %>
                            <% for(var itemIndex = 0; itemIndex < sellingListOpen.length; itemIndex++) {
                                var item = sellingListOpen[itemIndex];
                                var biddingDetails = biddingDetailListOpen[itemIndex];
                                var maxBid = 0;
                                var currency = '';
                                var maxBidIndex = -1;
                                if((biddingDetails != undefined && biddingDetails != null) && biddingDetails.length > 0) {
                                    for(var bidIndex = 0; bidIndex < biddingDetails.length; bidIndex++) {
                                        var bid = biddingDetails[bidIndex].bid.split(" ")[1];
                                        if(parseFloat(bid) > maxBid) {
                                            maxBid = parseFloat(bid);
                                            currency = biddingDetails[bidIndex].bid.split(" ")[0];
                                            maxBidIndex = bidIndex;
                                        }
                                    }
                                }
                                var formattedCreatedTime = item.createdAt.split('T')[1].split(".");
                            %>
                            <!-- Selling Items -->
                            <div class="item-selling-div fourteen columns">
                                <div class="item-selling-div">
                                    <div class="three columns">
                                        <% if(item.ItemImages.length > 0) {%>
                                        <img src="/uploads/items/<%= item.ItemImages[0].url %>"/>
                                        <% } %>
                                    </div>
                                    <div class="six columns">
                                        <h3><%= item.title %></h3>
                                        <p> Commodity Name: <%= item.Commodity.name %><br>
                                            <%if(maxBidIndex != -1) {%>
                                                Max Bid: <%= currency+" "+maxBid %> by <a href="/user/public/userId/<%= biddingDetails[maxBidIndex].UserId %>"> <%= biddingDetails[maxBidIndex].username %></a><br>
                                            <% } %>
                                            Status: Contract Created <br>
                                            <% if(remainingTimesOpen[itemIndex][0] != 0) {
                                                var remainingTime = remainingTimesOpen[itemIndex][0];
                                                var numdays = Math.floor(remainingTime / 86400);
                                                var numhours = Math.floor((remainingTime % 86400) / 3600);
                                                var numminutes = Math.floor(((remainingTime % 86400) % 3600) / 60);
                                                var numseconds = Math.floor(((remainingTime % 86400) % 3600) % 60);

                                                var formattedTime = "";
                                                formattedTime = numdays != 0 ? (formattedTime += numdays + " days ") : formattedTime;
                                                formattedTime = numhours != 0 ? (formattedTime += numhours + " hrs ") : formattedTime;
                                                formattedTime = numminutes != 0 ? (formattedTime += numminutes + " mins ") : formattedTime;
                                                formattedTime = numseconds != 0 ? (formattedTime += numseconds + " secs ") : formattedTime;
                                            %>
                                            <%= 'Time Left: '+formattedTime %><br>
                                            <% } else {%>
                                            <%= 'Bidding Closed At: ' + remainingTimesOpen[itemIndex][2] %><br>
                                            <% } %>
                                            Listed Date: <%= item.createdAt.split('T')[0]+" "+formattedCreatedTime[0] %><br>
                                        </p>
