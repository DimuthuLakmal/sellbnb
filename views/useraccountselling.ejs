                                                    <input type="hidden" name="itemId" value="<%= item.id %>" />
                                                    <input type="submit" class="submit" value="Yes"/>
                                                </div>
                                            </fieldset>
                                            <div class="clearfix"></div>
                                        </form>
                                    </div>
