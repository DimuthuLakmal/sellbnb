<body class="boxed">
<div id="wrapper">

    <% include header.ejs %>

<div class="row"></div>

    <!-- Content
    ================================================== -->

    <!-- Container -->
    <div class="container">

        <div class="six columns centered">

            <h3 class="headline">Recover Password</h3><span class="line" style="margin-bottom:20px;"></span>
            <div class="clearfix"></div>

            <form method="get" class="login" action="/api/user/forgotpassword_code_check">

                <% if(codeError != '' && codeError != undefined && codeError != null) { %>
                <div class="notification error closeable">
                    <p><span>Error!</span> <%= codeError %></p>
                    <a class="close" href="#"></a>
                </div>
                <% } %>

                <p class="form-row form-row-wide">
                    <label for="code">Code: </label>
                    <input type="text" class="input-text" name="code" id="code" value=""/>
                </p>

                <p class="form-row">
                    <input type="submit" class="button" name="next" value="Next"/>
                </p>

                <p class="lost_password">
                    <a href="/api/user/forgotpassword_code?email=<%= recoveryEmail %>">Resend Code</a>
                </p>

            </form>

        </div>
    </div>

    <div class="margin-top-50"></div>


    <!-- Footer
    ================================================== -->
    <% include footer.ejs %>

</div>


